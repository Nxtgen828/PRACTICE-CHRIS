# Server Configuration
NODE_ENV=development
PORT=8000

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/practice_db

# Redis Configuration
REDIS_URL=redis://localhost:6379

# API Keys (Add your actual keys)
API_KEY=your_api_key_here

# New Relic Monitoring
NEW_RELIC_LICENSE_KEY=your_license_key
NEW_RELIC_APP_NAME=Practice Chris API

# Docker Deployment Configuration
DOCKER_REGISTRY=ghcr.io
DOCKER_IMAGE_NAME=nxtgen828/practice-chris

# Kubernetes Configuration
KUBE_NAMESPACE=nxtgen-evo
KUBE_CLUSTER_NAME=nxtgen-production

# Container Registry
GITHUB_TOKEN=your_github_token
CONTAINER_REGISTRY_URL=ghcr.io

# Keeper Secrets Manager
KEEPER_SECRET_MANAGER_CONFIG=your_config
